{
  "name": "logic-expr-core",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./laws": {
      "import": "./dist/laws/index.mjs",
      "require": "./dist/laws/index.js"
    },
    "./transformations": {
      "import": "./dist/transformations/index.mjs",
      "require": "./dist/transformations/index.js"
    },
    "./expressionTree": {
      "import": "./dist/expressionTree/index.mjs",
      "require": "./dist/expressionTree/index.js"
    }
  },
  "version": "0.2.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc",
    "build": "tsup src/index.ts src/laws/index.ts src/transformations/index.ts src/expressionTree/index.ts --format cjs,esm --dts",
    "release major": "npm run build && npm version major && npm publish && git push --follow-tags",
    "release minor": "npm run build && npm version minor && npm publish && git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/larsfrs/logic-expr-core.git"
  },
  "author": "Lars Friese",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
